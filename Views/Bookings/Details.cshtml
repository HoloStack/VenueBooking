@model Booking
@{
    ViewData["Title"] = "Booking Details";
}
<h1>Booking on @Model.BookingDate.ToString("yyyy-MM-dd")</h1>
<dl class="row">
    <dt class="col-sm-2">Event</dt><dd class="col-sm-10">@Model.Event?.EventName</dd>
    <dt class="col-sm-2">Venue</dt><dd class="col-sm-10">@Model.Venue?.VenueName</dd>
</dl>
<p><a asp-action="Edit" asp-route-id="@Model.BookingId">Edit</a> | <a asp-action="Index">Back to List</a></p>
